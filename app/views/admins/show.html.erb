<% if !flash[:notice].blank? %>
	<div class = "notice">
	<%= flash[:notice] %>
	</div>
<% end %>

<%= link_to("<< Back to List", {:action => 'index'} , :class => 'back-to-link') %>

<div class="admins show">
<h2>Show Administrator</h2>

<table summary="Admin detail view">
	<tr>
		<th>E-Mail</th>
		<td><%= @admin.email %></td>
	</tr>
	<tr>
		<th>Name</th>
		<td><%= @admin.name %></td>
	</tr>
	<tr>
		<th>Created At</th>
		<td><%= @admin.created_at %></td>
	</tr>
	<tr>
		<th>Updated At</th>
		<td><%= @admin.updated_at %></td>
	</tr>
     </table>
</div>

	
